	<queryString language="SQL">
		<![CDATA[SELECT loja.tbl_sales.sale_date,
	loja.tbl_sales.total_value,
	loja.tbl_sales.the_amount,
	loja.tbl_sales.sale_id,
	loja.tbl_setting_of_system.name_company,
